{
  "context_menu": true,
  "auto_highlight": true,
  "auto_highlight_output_panel": [
    "exec"
  ],
  "log_list": {
    "example": {
      "type": "compile",
      "extension": [
        "*.log",
        ".vcs.log",
        ".cpp.log",
        "message*"
      ],
      "output.panel": [
        "exec"
      ],
      "use_link": true,
      "search_base": {
        "enable": true,
        "ignore_dir": [
          ""
        ],
        "max_scan_path": 1000
      },
      "bookmark": {
        "enable": true,
        "goto_error": true
      },
      "severity": {
        "error": {
          "enable": true,
          "pattern": [
            [
              "^Error-\\[",
              "^\\s*[\\n]"
            ],
            [
              "^{{{LINK}}}?[^\\r\\n]*?(?i)error",
              "[\\r\\n]"
            ],
            [
              "^\\w+:\\s*\\*E",
              "[\\r\\n]"
            ]
          ],
          "color": {
            "base": [
              "#F92672",
              ""
            ],
            "link": [
              "#E6DB74",
              ""
            ],
            "quote": [
              "#4F99D3",
              ""
            ]
          },
          "icon": "Danish Royalty/error.png"
        },
        "warning": {
          "enable": true,
          "pattern": [
            [
              "^WARN-\\[",
              "^\\s*[\\n]"
            ],
            [
              "^{{{LINK}}}?[^\\r\\n]*?(?i)warn",
              "[\\r\\n]"
            ],
            [
              "^\\w+:\\s*\\*W",
              "[\\r\\n]"
            ]
          ],
          "color": {
            "base": [
              "#A1B347",
              ""
            ],
            "link": [
              "#FD971F",
              ""
            ],
            "quote": [
              "#4F99D3",
              ""
            ]
          },
          "icon": "Danish Royalty/warning.png"
        }
      },
      "theme": {
        "foreground": "#D7D7D7",
        "caret": "#F29718",
        "selection": "#3A5166",
        "selectionBorder": "#181E26",
        "lineHighlight": "#283240"
      }
    }
  }
}
